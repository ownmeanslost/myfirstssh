<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.myfirstssh.resume.model.ModelPic" table="modelpic">
		<id name="Guid" type="string">
			<column name="Guid" />
			<generator class="identity" />
		</id>
		<property name="url" type="string" length="100" >
				<column name="url" >
                <comment>picurl</comment>
                </column>
		</property>
		<property name="resuumeGuid" type="string" length="100" >
				<column name="resuumeGuid" >
                <comment>mobandizhi</comment>
                </column>
		</property>
	</class>
</hibernate-mapping>
