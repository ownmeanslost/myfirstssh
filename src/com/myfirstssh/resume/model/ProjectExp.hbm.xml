<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.myfirstssh.resume.model.ProjectExp" table="projectexp">
		<id name="Guid" type="string">
			<column name="Guid" />
			<generator class="uuid" />
		</id>
		<property name="projectName" type="string" length="100" >
				<column name="projectName" >
                <comment>项目名称</comment>
                </column>
		</property>
		<property name="role" type="string" length="100" >
				<column name="role" >
                <comment>扮演角色</comment>
                </column>
		</property>
		<property name="startTime" type="string" length="100" >
				<column name="startTime" >
                <comment>开始时间</comment>
                </column>
		</property>
		<property name="lastTime" type="string" length="100" >
				<column name="lastTime" >
                <comment>结束时间</comment>
                </column>
		</property>
		<property name="projectType" type="string" length="100" >
				<column name="projectType" >
                <comment>项目类型</comment>
                </column>
		</property>
		<property name="tutor" type="string" length="100" >
				<column name="tutor" >
                <comment>指导人</comment>
                </column>
		</property>
		<property name="content" type="string" length="100" >
				<column name="content" >
                <comment>负责内容</comment>
                </column>
		</property>
		<property name="projectContent" type="string" length="100" >
				<column name="projectContent" >
                <comment>项目内容</comment>
                </column>
		</property>
		<property name="userGuid" type="string" length="100" >
				<column name="userGuid" >
                <comment>信息主键</comment>
                </column>
		</property>
	</class>
</hibernate-mapping>
